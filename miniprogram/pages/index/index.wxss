page {
  background-color: #DCE9F1;
  margin-bottom: 200rpx;
}
swiper {
  width: 95%;
  margin-left: 20rpx;
  margin-top: 20rpx;
  height: 370rpx;
}
swiper swiper-item navigator image {
  border-radius: 20rpx;
}
/* 功能服务*/
.navi-services {
  height: 253rpx;
  display: flex;
  flex-direction: column;
  background-color: white;
  margin-left: 20rpx;
  margin-right: 20rpx;
  margin-top: 10rpx;
  border-radius: 12rpx;
  box-shadow: 0rpx 2rpx 20rpx 0rpx rgba(204, 204, 204, 0.2);
}
.navi-services-title {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  margin-left: 24rpx;
  margin-right: 24rpx;
  margin-top: 24rpx;
}
.navi-services-title-left {
  font-size: 32rpx;
  color: #333;
  font-weight: 600;
}
.navi-services-title-right {
  font-size: 24rpx;
  color: #999;
}
.navi-services-list {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  margin-left: 24rpx;
  margin-right: 24rpx;
  margin-top: 32rpx;
}
.navi-services-list-item {
  display: flex;
  flex-direction: column;
  align-items: center;
}
.navi-services-list-item image {
  width: 60rpx;
  height: 60rpx;
}
.navi-services-list-item text {
  font-size: 26rpx;
}
/* 功能服务结束*/
/* 帮助服务开始*/
.navi-help {
  height: 317rpx;
  margin-left: 20rpx;
  margin-right: 20rpx;
  margin-top: 20rpx;
  display: flex;
  flex-direction: column;
  background-color: white;
  box-shadow: 0px 2px 20px 0px rgba(204, 204, 204, 0.2);
  border-radius: 12px;
}
.navi-help-notice {
  margin-top: 24rpx;
  margin-left: 24rpx;
  margin-right: 24rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
}
.navi-help-notice-preimg {
  width: 74rpx;
  height: 36rpx;
  background-image: url("https://6c69-lingju-2gmbkk11474c4790-1305134585.tcb.qcloud.la/images/index/home_notice.png?sign=6f22baf18c4d9303eec0455de2493eca&t=1616056852");
  background-size: 100% 100%;
  background-repeat: no-repeat;
  background-position: center;
}
.navi-help-notice-arrowimg {
  width: 12rpx;
  height: 20rpx;
  margin-left: 5rpx;
  background-image: url("https://6c69-lingju-2gmbkk11474c4790-1305134585.tcb.qcloud.la/images/index/arrow_right.png?sign=ab6364805bf4cee3d13fb6872990a99c&t=1616056894");
  background-size: 100% 100%;
  background-repeat: no-repeat;
  background-position: center;
}
.navi-help-notice-cont {
  display: flex;
  flex-direction: row;
  align-items: center;
}
.navi-help-notice-cont text {
  width: 420rpx;
  font-size: 28rpx;
  margin-left: 12rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.navi-help-notice-cont-type {
  width: 94rpx;
  margin-left: 12rpx;
  margin-top: 12rpx;
  margin-bottom: 12rpx;
  background-color: #FFECE3;
  display: flex;
  align-items: center;
  border-radius: 6rpx;
}
.navi-help-notice-cont-type text {
  font-size: 18rpx;
  color: #FF6010;
}
.common-line {
  width: 100%;
  height: 1rpx;
  background: #EEEEEE;
  margin-left: 24rpx;
  margin-right: 24rpx;
  margin-top: 24rpx;
}
.navi-help-list {
  margin-left: 24rpx;
  margin-right: 24rpx;
  margin-top: 20rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}
.navi-help-list-guide {
  width: 308rpx;
  height: 168rpx;
  background: #F5F5F5;
  border-radius: 4px;
  opacity: 0.9;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-evenly;
}
.navi-help-list-guide-desc {
  display: flex;
  flex-direction: column;
  align-items: flex-start;
  margin-left: 10rpx;
}
.navi-help-list-guide-desc text:nth-child(1) {
  color: #333;
  font-weight: 600;
  font-size: 30rpx;
}
.navi-help-list-guide-desc text:nth-child(2) {
  color: #999;
  font-size: 22rpx;
  margin-top: 10rpx;
}
.navi-help-list-guide-img {
  width: 145rpx;
  height: 103rpx;
  background-image: url("https://6c69-lingju-2gmbkk11474c4790-1305134585.tcb.qcloud.la/images/index/new_user_guide.png?sign=4d5080c0afe362112241c4644b7f0e93&t=1616056960");
  background-size: 100% 100%;
  background-repeat: no-repeat;
  background-position: center;
}
.navi-help-list-wy {
  width: 308rpx;
  height: 168rpx;
  background: #F5F5F5;
  border-radius: 4px;
  opacity: 0.9;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-evenly;
}
.navi-help-list-wy-desc {
  display: flex;
  flex-direction: column;
  align-items: flex-start;
}
.navi-help-list-wy-desc text:nth-child(1) {
  color: #333;
  font-weight: 600;
  font-size: 30rpx;
}
.navi-help-list-wy-desc text:nth-child(2) {
  color: #999;
  font-size: 22rpx;
  margin-top: 10rpx;
}
.navi-help-list-wy-img {
  width: 75rpx;
  height: 138rpx;
  margin-top: 15rpx;
  background-image: url("https://6c69-lingju-2gmbkk11474c4790-1305134585.tcb.qcloud.la/images/index/contact_wuye.png?sign=aaf58b5dc8756c1252ea480961c8f7fc&t=1616056980");
  background-size: 100% 100%;
  background-repeat: no-repeat;
  background-position: center;
}
/* 帮助服务结束*/
/* 通知公告栏*/
.news {
  margin-left: 32rpx;
  margin-right: 32rpx;
  margin-top: 20rpx;
  display: flex;
  flex-direction: column;
}
.news-head {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}
.news-head > text {
  font-size: 34rpx;
  color: #333;
  font-weight: 600;
}
.news-head > text {
  font-size: 34rpx;
  color: #333;
  font-weight: 600;
}
.news-head > view {
  display: flex;
  flex-direction: row;
  align-items: center;
}
.news-head > view text {
  font-size: 24rpx;
  color: #999;
}
.news-head > view image {
  width: 12rpx;
  height: 20rpx;
  margin-left: 12rpx;
}
/* 帖子*/
.new_wrap {
  width: 95%;
  height: 400rpx;
  background-color: #fff;
  border-radius: 15rpx;
  margin-left: 17rpx;
  margin-top: 25rpx;
  padding-top: 20rpx;
}
.new_wrap .item_wrap {
  bottom: 5rpx;
  display: flex;
  flex-wrap: wrap;
  flex-direction: row;
  position: relative;
}
.new_wrap .item_wrap .image_wrap {
  flex: 1;
  position: relative;
}
.new_wrap .item_wrap .image_wrap image {
  margin-left: 5rpx;
  margin-top: 0rpx;
  width: 230rpx;
  height: 230rpx;
  border-radius: 8rpx;
}
.new_wrap .item_wrap .text_wrap {
  flex: 4;
  display: flex;
  flex-direction: column;
}
.new_wrap .item_wrap .text_wrap .content {
  margin-right: 10rpx;
  display: -webkit-box;
  overflow: hidden;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  font-size: 30rpx;
  color: #111;
}
.new_wrap .item_wrap .text_wrap .date {
  margin-top: 35rpx;
  font-size: 22rpx;
  color: #555;
}
